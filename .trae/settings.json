{
  "trae.fileAssociations": {
    "*.mega": "omega"
  },
  "trae.fileIcons": {
    "mega": {
      "iconPath": "omega-vscode-extension/images/omega-icon.png",
      "theme": {
        "light": "omega-vscode-extension/images/omega-icon.png",
        "dark": "omega-vscode-extension/images/omega-icon.png"
      }
    }
  },
  "trae.languages": {
    "omega": {
      "extensions": [".mega"],
      "displayName": "OMEGA Blockchain Language",
      "icon": "./omega-vscode-extension/images/omega-file-icon-mega.svg",
      "mimeType": "text/x-omega"
    }
  },
  "editor.fileIconTheme": "omega-icons",
  "workbench.iconTheme": "omega-icons",
  "files.associations": {
    "*.mega": "omega"
  },
  "workbench.editorAssociations": {
    "*.mega": "default"
  }
}