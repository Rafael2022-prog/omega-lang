// Data structures for testing framework
struct TestSuite {
    string name;
    string description;
    TestType[] test_types;
    uint256 target_coverage;
    uint256 execution_order;
    uint256 timeout_seconds;
    bool parallel_execution;
    bool retry_on_failure;
    uint256 max_retries;
    bool required;
    bool blocking;
    TestCase[] test_cases;
}

struct TestCase {
    string name;
    string category;
    TestPriority priority;
    uint256 execution_time_estimate;
    string required_state;
    string expected_result;
    string[] assertions;
    mapping(string => string) parameters;
}

struct TestResult {
    string suite_name;
    uint256 execution_start;
    uint256 execution_end;
    uint256 total_execution_time;
    TestStatus status;
    uint256 test_cases_passed;
    uint256 test_cases_failed;
    uint256 test_cases_skipped;
    string[] error_messages;
    uint256 gas_used;
    uint256 coverage_percentage;
    mapping(string => uint256) performance_metrics;
}

struct TestConfiguration {
    string network;
    string gas_price;
    uint256 gas_limit;
    uint256 timeout;
    uint256 max_concurrent_transactions;
    uint256 test_data_seed;
    bool enable_logging;
    bool save_state_snapshots;
    bool reset_state_between_tests;
    bool coverage_reporting;
    bool performance_profiling;
}

struct TestExecutionSummary {
    uint256 total_suites;
    uint256 passed_suites;
    uint256 failed_suites;
    uint256 total_execution_time;
    uint256 start_time;
    uint256 end_time;
    bool overall_success;
    mapping(string => uint256) performance_metrics;
    mapping(string => uint256) resource_usage;
}

struct TestPermission {
    bool can_execute_tests;
    bool can_modify_test_suites;
    bool can_view_test_results;
    bool can_configure_testing;
    string[] allowed_test_types;
    uint256 max_execution_time;
    uint256 max_concurrent_tests;
}

enum TestType {
    Unit,
    Integration,
    Security,
    Performance,
    CrossChain,
    Fuzz,
    Property,
    Mutation,
    Load,
    Chaos
}

enum TestPriority {
    Low,
    Medium,
    High,
    Critical
}

enum TestStatus {
    NotStarted,
    Running,
    Passed,
    Failed,
    Warning,
    Skipped,
    Error
}