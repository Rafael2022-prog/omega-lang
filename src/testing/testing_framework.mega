    // Run all test suites
    function run_all_tests(string config_name) public returns (TestExecutionSummary) {
        TestExecutionSummary memory summary;
        summary.total_suites = 0;
        summary.passed_suites = 0;
        summary.failed_suites = 0;
        summary.total_execution_time = 0;
        summary.start_time = block.timestamp;
        
        // Execute test suites in order
        string[] memory suite_names = _get_test_suite_names_in_order();
        
        for (uint256 i = 0; i < suite_names.length; i++) {
            string memory suite_name = suite_names[i];
            
            if (test_suites[suite_name].required) {
                TestResult memory result = execute_test_suite(suite_name, config_name);
                
                summary.total_suites++;
                
                if (result.status == TestStatus.Passed) {
                    summary.passed_suites++;
                } else if (result.status == TestStatus.Failed) {
                    summary.failed_suites++;
                }
                
                summary.total_execution_time += result.total_execution_time;
            }
        }
        
        summary.end_time = block.timestamp;
        summary.overall_success = (summary.failed_suites == 0);
        
        return summary;
    }
    
    // Get test suite names in execution order
    function _get_test_suite_names_in_order() private returns (string[]) {
        // Return suite names sorted by execution_order
        return [
            "unit_tests",
            "integration_tests",
            "security_tests",
